{
  "name": "node-webdollar",
  "version": "0.1.2",
  "description": "Node of WebDollar",
  "main": "index.js",
  "scripts": {
    "build_terminal": "webpack --config build_webpack/webpack.terminal.config.js",
    "build_terminal_test": "webpack --config build_webpack/tests/webpack.terminal.test.config.js",
    "build_terminal_menu": "webpack --config build_webpack/webpack.terminal.menu.config.js",
    "build_browser": "webpack --config build_webpack/webpack.browser.config.js",
    "build_browser_user_interface": "cross-env WEBPACK_TYPE=user_interface webpack --config build_webpack/webpack.browser.config.js ",
    "show_browser_test": "node src/tests/browser/show-browser.test.js",
    "start": "webpack --config build_webpack/webpack.terminal.config.js && node --max_old_space_size=4096  dist_bundle/terminal-bundle.js",
    "start_force_load": "cross-env FORCE_LOAD=1 webpack --config build_webpack/webpack.terminal.config.js && node --max_old_space_size=4096  dist_bundle/terminal-bundle.js",
    "debug": "webpack --config build_webpack/webpack.terminal.config.js && node --max_old_space_size=4096 --inspect=0.0.0.0:9229 dist_bundle/terminal-bundle.js ",
    "start_mining": "webpack --config build_webpack/webpack.terminal-mining.config.js && node dist_bundle/terminal-bundle.js ",
    "test": "npm run build_terminal_test && mocha --exit --timeout 10000 dist_bundle/terminal-mocha-bundle.js --reporter mochawesome && npm run show_mocha_report",
    "test_browser": "webpack --config build_webpack/tests/webpack.browser.test.config.js && npm run show_browser_test",
    "show_mocha_report": "node src/tests/browser/show-mocha.test.js",
    "coverage": "npm run build_terminal  && istanbul cover _mocha dist/tests/main.test.js -x *.test.js && npm run show_coverage",
    "show_coverage": "node src/tests/browser/show-coverage.test.js",
    "build_terminal_worker": "webpack --config build_webpack/webpack.terminal-worker.config.js",
    "commands": "npm run build_terminal_menu && npm run build_terminal_worker && node --max_old_space_size=6144 dist_bundle/terminal-menu-bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebDollar/Node-WebDollar.git"
  },
  "keywords": [
    "p2p",
    "Blockchain",
    "Digital Currency",
    "Crypto Currency",
    "Sockets",
    "webrtc"
  ],
  "author": "WebDollar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WebDollar/Node-WebDollar/issues"
  },
  "homepage": "https://github.com/WebDollar/Node-WebDollar#readme",
  "dependencies": {
    "argon2": "^0.19.3",
    "argon2-browser": "^1.1.0",
    "atob": "^2.0.3",
    "axios": "^0.16.2",
    "babel-plugin-module-alias": "^1.6.0",
    "better-console": "^1.0.1",
    "big-integer": "^1.6.26",
    "bignumber.js": "^6.0.0",
    "bitcoinjs-lib": "^3.3.2",
    "blob-to-buffer": "^1.2.7",
    "bs58": "^4.0.1",
    "btoa": "^1.1.2",
    "buffer": "^5.1.0",
    "console-error": "0.0.4",
    "console-info": "0.0.5",
    "console-warn": "0.0.4",
    "cors": "^2.8.4",
    "events": "^2.0.0",
    "fkill": "^5.3.0",
    "fs": "0.0.1-security",
    "get-random-values": "^1.2.0",
    "https": "^1.0.0",
    "ipaddr.js": "^1.6.0",
    "is-array-buffer": "^1.0.0",
    "md5": "^2.2.1",
    "my-ip": "^1.1.1",
    "node-gyp": "^3.6.2",
    "opn": "^5.2.0",
    "path": "^0.12.7",
    "pem": "^1.12.4",
    "pouchdb": "^6.4.3",
    "pouchdb-node": "^6.4.3",
    "public-ip": "^2.4.0",
    "readline": "^1.3.0",
    "safe-compare": "^1.1.2",
    "sanitizer": "^0.1.3",
    "save": "^2.3.2",
    "socket.io": "1.7.4",
    "socket.io-client": "1.7.4",
    "threads": "^0.10.1",
    "tweetnacl": "^1.0.0",
    "uuid": "^3.2.1",
    "webworkify-webpack": "^2.1.1",
    "x509": "^0.3.3",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2017": "^6.24.1",
    "babelify": "^7.3.0",
    "compression-webpack-plugin": "^1.1.9",
    "cross-env": "^5.1.3",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0",
    "mocha-loader": "^1.1.3",
    "mocha-webpack": "^1.0.1",
    "mochawesome": "^2.3.1",
    "timers-browserify": "^2.0.6",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "optionalDependencies": {
    "bufferutil": "^3.0.0",
    "utf-8-validate": "2.0.0"
  }
}
